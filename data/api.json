[
	{
		"path": "/products/*",
		"data": true,
		"auth": {
			"validator": "$auth != null"
		}
	},
	{
		"path": "/cart/*",
		"data": true,
		"parameters": {
			"filter": {
				"value": "{\"userId\":{\"operator\":\"=\",\"value\": $data.userId }"
			}
		},
		"auth": {
			"validator": "$auth != null"
		}
	}
]